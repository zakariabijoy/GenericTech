{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "APISettings": {
    "GatewayAddress": "http://localhost:8010"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "warning"
      }
    }
  },
  "ElasticConfiguration": {
    "Uri": "http://host.docker.internal:9200"
  }
}
